<Window x:Class="ПримерДляДЭ.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ПримерДляДЭ"
        mc:Ignorable="d"
        Title="Салон" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50*"></RowDefinition>
            <RowDefinition Height="200*"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid Background="#FFE1E4FF"></Grid>
        <Image HorizontalAlignment="Left" Margin="10,0,0,0" Width="55" Source="Resurs/beauty_logo.png" ></Image>
        <Button x:Name="btnBack" Click="btnBack_Click" Width="150" Height="30" FontFamily="Tahoma"
                FontSize="16" Content="Назад" Foreground="White" Background="#FFFF4A6D" Margin="500,0,0,0" />
        <Frame x:Name="frmMain" Grid.Row="1" />

    </Grid>
</Window>
